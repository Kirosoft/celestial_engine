{
  "$id": "nodes/GitHubInput.schema.json",
  "title": "GitHubInput",
  "type": "object",
  "properties": {
    "id": { "type": "string" },
    "type": { "const": "GitHubInput" },
    "name": { "type": "string" },
    "position": { "type": "object", "properties": {"x":{"type":"number"},"y":{"type":"number"}}, "required": ["x","y"] },
    "props": {
      "type": "object",
      "properties": {
        "repo": { "type": "string" },
        "path": { "type": "string" },
        "branch": { "type": "string" }
      },
      "required": ["repo"],
      "additionalProperties": true
    }
  },
  "required": ["id", "type", "name", "props"],
  "additionalProperties": true
}
